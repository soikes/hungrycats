<div id='remaining_container' class='widget'>
  
  <span class='widget_title'>remaining food</span>
  <div class='row'>
    remaining food on device 'penny_feeder'
    <div id="remaining">
      <div class="anchor" style="width: 100%">
        <% if 100 < 30 %>
          <span class="inner lowfood"></span>
        <% else %>
          <span class="inner"></span>
        <% end %>
      </div>
    </div>
  </div>
  
  <div class='row'>
    remaining food on device 'scout_feeder'
    <div id="remaining">
      <div class="anchor" style="width: 20%">
        <% if 20 < 30 %>
          <span class="inner lowfood"></span>
        <% else %>
          <span class="inner"></span>
        <% end %>
      </div>
    </div>
  </div>
  
</div>
